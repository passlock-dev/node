{
  "name": "@passlock/node",
  "type": "module",
  "version": "0.9.2",
  "description": "Server side passkey library for node/express",
  "keywords": [
    "passkey",
    "passkeys",
    "webauthn",
    "node",
    "express"
  ],
  "author": {
    "name": "Toby Hobson",
    "email": "toby@passlock.dev"
  },
  "license": "MIT",
  "homepage": "https://passlock.dev",
  "repository": "github.com/passlock-dev/node",
  "bugs": {
    "url": "https://github.com/passlock-dev/node/issues",
    "email": "team@passlock.dev"
  },
  "scripts": {
    "clean": "tsc --build --clean",
    "build": "tsc --build",
    "test": "vitest run",
    "test:watch": "vitest dev",
    "test:ui": "vitest --coverage.enabled=true --ui",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "lint": "eslint --ext .ts src",
    "lint:fix": "pnpm run lint --fix",
    "format": "prettier --write \"src/**/*.+(js|ts|json)\"",
    "ncu": "ncu -x @passlock/shared",
    "ncu:save": "ncu -u -x @passlock/shared"
  },
  "dependencies": {
    "@passlock/shared": "workspace:^",
    "effect": "2.4.16"
  },
  "devDependencies": {
    "@tsconfig/node16": "^16.1.3",
    "@types/node": "^20.12.2",
    "@typescript-eslint/eslint-plugin": "^7.5.0",
    "@typescript-eslint/parser": "^7.5.0",
    "@vitest/coverage-v8": "^1.4.0",
    "@vitest/ui": "^1.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3",
    "vite": "^5.2.7",
    "vitest": "^1.4.0",
    "vitest-mock-extended": "^1.3.1"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "src",
    "dist"
  ]
}
